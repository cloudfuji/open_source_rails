<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en" class="no-js"> <!--<![endif]--> 
  <head>  
    <title>Open Source Rails</title> 
    <%= stylesheet_link_tag :reset_styles %>
    <%= stylesheet_link_tag :main %>
    <link href="/colorbox/example2/colorbox.css" media="screen" rel="stylesheet" type="text/css" /> 
    <%= javascript_include_tag :application %>
    <%= javascript_include_tag "jquery.easing.1.3.js" %>
    <%= javascript_include_tag "jquery.slides.min.js" %>
    <%= javascript_include_tag "jquery.autobox.js" %>
    <%= javascript_include_tag "jquery.validationEngine-en.js", "jquery.validationEngine.js" %>
    <%= stylesheet_link_tag "validationEngine.jquery.css" %>
    <script type="text/javascript" src="/colorbox/colorbox/jquery.colorbox.js"></script>

    <%= yield(:head) %>
    
  </head>
  <body class="no_background">
      <div id="main" class="no_margin">
  	    <%= yield %>
      </div>
  </body>
</html>
